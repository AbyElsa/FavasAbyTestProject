{
  "expo": {
    "name": "PHORVIA",
    "slug": "phorvia",
    "privacy": "unlisted",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "default",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/applogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}